<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #F1F1E6;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spachecor.ejerciciofinalsgecn.controller.gestion.GestionarCursosController">
   <children>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="1240.0">
         <children>
            <Label fx:id="eduTrackLabel" text="EduTrack" textFill="#02547f">
               <font>
                  <Font size="24.0" />
               </font></Label>
         </children></VBox>
      <VBox alignment="CENTER" prefHeight="530.0" prefWidth="1240.0">
         <children>
            <TableView fx:id="tablaCursosTableView" prefHeight="530.0" prefWidth="1240.0" style="-fx-background-color: #ECFBFF;" />
         </children></VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="100.0">
         <children>
            <Label fx:id="errorLabel" textFill="#6f0000">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="1240.0" spacing="250.0">
         <children>
            <Button fx:id="nuevoButton" mnemonicParsing="false" onAction="#onClickNuevoButton" style="-fx-background-color: #ECFBFF; -fx-border-color: #02547F; -fx-background-radius: 10; -fx-border-radius: 10;" text="Nuevo" textFill="#02547f">
               <font>
                  <Font size="18.0" />
               </font></Button>
            <Button fx:id="salirButton" mnemonicParsing="false" onAction="#onClickSalirButton" style="-fx-background-color: #ECFBFF; -fx-border-color: #02547F; -fx-background-radius: 10; -fx-border-radius: 10;" text="Salir" textFill="#02547f">
               <font>
                  <Font size="18.0" />
               </font></Button>
         </children></HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
