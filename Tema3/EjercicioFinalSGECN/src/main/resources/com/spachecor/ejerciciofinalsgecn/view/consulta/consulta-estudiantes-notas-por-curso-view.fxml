<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #F1F1E6;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spachecor.ejerciciofinalsgecn.controller.consulta.ConsultaEstudiantesNotasPorCursoController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <VBox alignment="CENTER" prefHeight="680.0" prefWidth="440.0" spacing="25.0">
            <children>
                <Label text="Curso" textFill="#02547f">
               <font>
                  <Font size="18.0" />
               </font></Label>
                <ChoiceBox fx:id="cursoChoiceBox" prefWidth="150.0">
                    <VBox.margin>
                        <Insets bottom="25.0" />
                    </VBox.margin>
                </ChoiceBox>
            <Button mnemonicParsing="false" onAction="#onClickVolverButton" text="Volver" textFill="#02547f">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
               <font>
                  <Font size="18.0" />
               </font></Button>
            </children>
        </VBox>
        <VBox alignment="CENTER" prefHeight="680.0" prefWidth="800.0">
            <children>
                <TableView fx:id="tablaTableView" prefHeight="680.0" prefWidth="800.0" />
            </children>
        </VBox>
    </children>
</HBox>
