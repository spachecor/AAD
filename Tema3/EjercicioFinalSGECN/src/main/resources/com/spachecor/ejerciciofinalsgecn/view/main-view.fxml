<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spachecor.ejerciciofinalsgecn.controller.MainController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <VBox alignment="CENTER" prefHeight="100.0" prefWidth="1240.0">
         <children>
            <Label text="EduTrack" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="580.0" prefWidth="1240.0" spacing="40.0">
         <children>
            <Button fx:id="gestionarCursosButton" mnemonicParsing="false" onAction="#onClickGestionarCursosButton" text="Gestionar Cursos">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="gestionarEstudiantesButton" mnemonicParsing="false" onAction="#onClickGestionarEstudiantesButton" text="Gestionar Estudiantes">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="gestionarNotasButton" mnemonicParsing="false" onAction="#onClickGestionarNotasButton" text="Gestionar Notas">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="buscadorButton" mnemonicParsing="false" onAction="#onClickBuscadorButton" text="Buscador">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
