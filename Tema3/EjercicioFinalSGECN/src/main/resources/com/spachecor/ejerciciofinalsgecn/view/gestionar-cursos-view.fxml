<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spachecor.ejerciciofinalsgecn.controller.GestionarCursosController">
   <children>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="1240.0">
         <children>
            <Label text="EduTrack" />
         </children></VBox>
      <VBox alignment="CENTER" prefHeight="530.0" prefWidth="1240.0">
         <children>
            <TableView fx:id="tablaCursosTableView" prefHeight="530.0" prefWidth="1240.0" />
         </children></VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="100.0">
         <children>
            <Label fx:id="errorLabel" />
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="1240.0" spacing="250.0">
         <children>
            <Button mnemonicParsing="false" text="Nuevo" />
            <Button fx:id="salirButton" mnemonicParsing="false" onAction="#onClickSalirButton" text="Salir" />
         </children></HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
